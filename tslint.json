{
  "cliOptions": {
    "exclude": [
      "src/generated/**/*.ts"
    ]
  },
  "extends": ["tslint:latest", "tslint-config-standard"],
  "rules": {
    "interface-name": false,
    "no-console": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
